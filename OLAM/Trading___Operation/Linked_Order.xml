                    <description></description>
                    <creationDate>2016-11-23T02:18:13.743+08:00</creationDate>
                    <updateDate>2015-09-16T17:20:08.580+08:00</updateDate>
                    <language>sql</language>
                    <queryString>SELECT 'ALL' AS LABEL,
       '%'   AS VALUE, 1 AS SEQ
UNION
SELECT DISTINCT PCNAME,
                PCNAME,
                2 AS SEQ
FROM   JCPROFITCENTER A
LEFT JOIN JCPROFITCENTERGROUP B
ON A.PCGROUPID = B.GROUPID
WHERE COALESCE(B.SHORTNAME, '%') LIKE $P{profit_center_group_p}
AND A.ISACTIVE=1
AND COALESCE(B.ISACTIVE, 1) =1
ORDER BY SEQ, LABEL</queryString>
                    <dataSource>
                        <uri>/organizations/organization_1/standard/datasource/JNDIcxcdata</uri>
                    </dataSource>
                </localResource>
