	<queryString>
		<![CDATA[SELECT Y.CONTRACT_BOOK
		,asofdate
		 ,(SELECT TOP 1 EOD_DATE FROM PNL_ATTRIBUTION_DETAIL_TABLE where EOD_DATE < $P{asofdate} and CONTRACT_BOOK =$P{ctrbook}  ORDER BY EOD_DATE DESC) as 'p_asofdate'
FROM (SELECT DISTINCT
		  DPA.CONTRACT_BOOK CONTRACT_BOOK,
		  DPA.EOD_DATE asofdate
		FROM PNL_ATTRIBUTION_DETAIL_TABLE DPA
		JOIN (SELECT MAX(EOD_DATE) EOD_DATE
				FROM PNL_ATTRIBUTION_DETAIL_TABLE b
				WHERE b.EOD_DATE <= $P{asofdate}
				AND $X{IN,b.PROFIT_CENTER,pc}
				AND $X{IN, b.COMMODITY , commodity}
				AND b.CONTRACT_BOOK like $P{ctrbook}
				) BVBV
		ON DPA.EOD_DATE = BVBV.EOD_DATE
		AND $X{IN,DPA.PROFIT_CENTER,pc}
		AND $X{IN,DPA.COMMODITY ,commodity}
		AND DPA.CONTRACT_BOOK like $P{ctrbook}

	) Y
WHERE 0 = 0]]>
	</queryString>
