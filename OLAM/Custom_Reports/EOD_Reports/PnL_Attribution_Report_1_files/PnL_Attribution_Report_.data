	<subDataset name="CrossTab" uuid="f9795a1b-014f-41a6-9061-079a0345b6cb">
		<parameter name="ctrbook" class="java.lang.String"/>
		<parameter name="asofdate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="instr" class="java.util.Collection"/>
		<parameter name="pc_g" class="java.util.Collection"/>
		<parameter name="pc" class="java.util.Collection"/>
		<parameter name="commodity" class="java.util.Collection"/>
		<parameter name="Report_By" class="java.lang.String"/>
		<parameter name="Then_By" class="java.lang.String"/>
		<parameter name="p_asofdate" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select [PC],
	report_COMMODITY as[COMMODITY],

case when report_instr in ('PHYSICAL','IDT','PAPER') then 'PHYSICAL'
else report_instr end as[INSTR],
	[STRATEGY],
	[PRICE INDEX],
isnull([Realised_PnL],0)+isnull([OpenFairValue],0) as 'PNL_Value',
	[CHANGE_GROUP]as 'CHANGE_TYPE' ,
	'PnL' as 'PNL_TYPE'
	,case when [CHANGE_GROUP] = 'Delta PnL' then '1'
	when [CHANGE_GROUP] = 'New Contract' then '2'
	when [CHANGE_GROUP] = 'Market Price' then '3'
	when [CHANGE_GROUP] = 'DC' then '4'
	when [CHANGE_GROUP] = 'Fx' then '5'
	when [CHANGE_GROUP] = 'Others' then '6'
	 when CHANGE_GROUP like'Posn as of %' and
(cast(convert(varchar(10),left(EOD_DATE,4)+''+
right(CHANGE_GROUP,6) ,121)as date ) < EOD_DATE) then '-1'
when [CHANGE_GROUP] = 'New Posn' then '-2'
 when CHANGE_GROUP like'Posn as of %' and
(cast(convert(varchar(10),left(EOD_DATE,4)+''+
right(CHANGE_GROUP,6) ,121)as date ) = EOD_DATE) then '-3'


	else '8' end as 'orderbyChanges'
from PNL_ATTRIBUTION_DETAIL_TABLE
where contract_book=$P{ctrbook}
and eod_date=$P{asofdate}
and PRIOREODDATE=$P{p_asofdate}


----------------
union all

select   [PC],
	report_COMMODITY as[COMMODITY],
	case when report_instr in ('PHYSICAL','IDT','PAPER') then 'PHYSICAL'
else report_instr end as[INSTR],
	[STRATEGY],
	[PRICE INDEX],
	(case when [CHANGE_GROUP] ='Delta PnL' then
isnull([Realised_PnL],0)+isnull([OpenFairValue],0) else 0 end
-
	case when [CHANGE_GROUP] not in ('Delta PnL','New Posn' ) and
	CHANGE_GROUP not like'Posn as of %' then
isnull([Realised_PnL],0)+isnull([OpenFairValue],0) else 0 end
) as 'PNL_Value',
'Others Unexplained' as 'CHANGE_TYPE',
'PnL' as 'PNL_TYPE',

 '8'   as 'orderbyChanges'

from PNL_ATTRIBUTION_DETAIL_TABLE

where contract_book=$P{ctrbook}
and eod_date=$P{asofdate}
and PRIOREODDATE=$P{p_asofdate}]]>
		</queryString>
