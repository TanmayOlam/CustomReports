		<queryString>
			<![CDATA[SELECT COMP.COMPANY_NAME     AS COMPANY,
       PF.PROFIT_CENTER_NAME AS PROFIT_CENTER,
       F.CONTRACT_REFERENCE,
       Format(F.CONTRACT_DATE,'dd-MMM-yy') AS CONTRACT_DATE,
       F.BROKER,
       F.COMMODITYID         AS COMMODITY,
       'Futures'             AS INSTRUMENT_TYPE,
       F.CONTRACT_TERM                 AS CONTRACT_TERM,
       F.SHIPMENT_MONTH      AS TERMINAL_MONTH,

	   CASE
WHEN F.LONG_SHORT = 'LONG' THEN F.NO_OF_LOTS
WHEN F.LONG_SHORT = 'SHORT' THEN F.NO_OF_LOTS
end as NO_OF_LOT,

	   CASE WHEN  F.LONG_SHORT = 'LONG' THEN F.UNIT_PRICE
	   When    F.LONG_SHORT = 'SHORT' THEN F.UNIT_PRICE
end as UNIT_FLAT_PRICE,
	NULL AS CALL_PUT,
	NULL AS STRIKE,
       F.LONG_SHORT,
     F.NO_OF_LOTS,
	 F.BROKER_ACCOUNT,
	 F.CONTRACT_STATUS

FROM   BI_VIEW_CONTRACT_FUTURE F
       JOIN BI_VIEW_COUNTERPARTY CP
         ON F.CPID = CP.CPID
       JOIN BI_VIEW_COMPANY COMP
         ON F.COMPANYID = COMP.COMPANYID
       JOIN BI_VIEW_CONTRACT_BOOK CB
         ON F.CTRBOOKID = CB.CTRBOOKID
       JOIN BI_VIEW_PROFITCENTER PF
         ON F.PFID = PF.PFID
     WHERE F.CONTRACT_STATUS IN ( 'Confirmed' , 'Fulfilled')

	 and Case when SUBSTRING(F.CONTRACT_REFERENCE,CHARINDEX('/',F.CONTRACT_REFERENCE,4 )+1,Len(F.CONTRACT_REFERENCE))
     not like  '%[^0-9]%'  then 0 else 1  end = 0
     AND $X{[BETWEEN], F.CONTRACT_DATE, contract_from_date, contract_to_date}
     AND COMP.COMPANY_NAME LIKE  $P{company}
     AND CB.CONTRACT_BOOK_NAME LIKE  $P{ctrbook}
     AND PF.PROFIT_CENTER_NAME LIKE  $P{profitcenter}
     AND F.TRADERID LIKE  $P{trader}
     AND F.BROKER LIKE  $P{broker}
     AND $X{IN, F.BROKER_ACCOUNT, broker_account}
     AND INSTRUMENT_TYPE LIKE  $P{ctrtype}
     AND F.CONTRACT_TERM LIKE  $P{ctrterm}
     $P!{QSTR_Filter}

UNION
SELECT COMP.COMPANY_NAME     AS COMPANY,
       PF.PROFIT_CENTER_NAME AS PROFIT_CENTER,
       O.CONTRACT_REFERENCE,
       Format(O.CONTRACT_DATE,'dd-MMM-yy') AS CONTRACT_DATE,
       O.BROKER,
       O.COMMODITYID         AS COMMODITY,
       'Options'             AS INSTRUMENT_TYPE,
       O.OPTION_TERM               AS CONTRACT_TERM,
       O.SHIPMENT_MONTH      AS TERMINAL_MONTH,

	   CASE
WHEN O.LONG_SHORT = 'LONG' THEN O.NO_OF_LOTS
WHEN O.LONG_SHORT = 'SHORT' THEN O.NO_OF_LOTS
end as NO_OF_LOT,

	  CASE
WHEN  O.LONG_SHORT = 'LONG' THEN O.UNIT_PRICE
WHEN O.LONG_SHORT = 'SHORT' THEN O.UNIT_PRICE
end as UNIT_FLAT_PRICE,

	  CASE
WHEN  O.LONG_SHORT = 'SHORT' THEN O.CALL_PUT
WHEN O.LONG_SHORT = 'LONG' THEN O.CALL_PUT
end as CALL_PUT,

  CASE
WHEN   O.LONG_SHORT = 'LONG' THEN O.STRIKE_PRICE
WHEN   O.LONG_SHORT = 'SHORT' THEN O.STRIKE_PRICE
end as STRIKE,
       O.LONG_SHORT,
     O.NO_OF_LOTS,
	 O.BROKER_ACCOUNT,
	 O.CONTRACT_STATUS

FROM   BI_VIEW_CONTRACT_OPTION O
       JOIN BI_VIEW_COUNTERPARTY CP
         ON O.CPID = CP.CPID
       JOIN BI_VIEW_COMPANY COMP
         ON O.COMPANYID = COMP.COMPANYID
       JOIN BI_VIEW_CONTRACT_BOOK CB
         ON O.CTRBOOKID = CB.CTRBOOKID
       JOIN BI_VIEW_PROFITCENTER PF
         ON O.PFID = PF.PFID
     WHERE O.CONTRACT_STATUS IN ( 'Confirmed' , 'Fulfilled')

	 and Case when SUBSTRING(O.CONTRACT_REFERENCE,CHARINDEX('/',O.CONTRACT_REFERENCE,4 )+1,Len(O.CONTRACT_REFERENCE))
     not like  '%[^0-9]%'  then 0 else 1  end = 0

     AND $X{[BETWEEN], O.CONTRACT_DATE, contract_from_date, contract_to_date}
     AND COMP.COMPANY_NAME LIKE  $P{company}
     AND CB.CONTRACT_BOOK_NAME LIKE  $P{ctrbook}
     AND PF.PROFIT_CENTER_NAME LIKE  $P{profitcenter}
     AND O.TRADERID LIKE  $P{trader}
     AND O.BROKER LIKE  $P{broker}
     AND $X{IN, O.BROKER_ACCOUNT, broker_account}
     AND INSTRUMENT_TYPE LIKE  $P{ctrtype}
     AND O.CONTRACT_TERM LIKE  $P{ctrterm}
     AND COALESCE(O.CALL_PUT,'') LIKE $P{trade_type}
     $P!{QSTR_Filter}


ORDER BY
BROKER,BROKER_ACCOUNT,
INSTRUMENT_TYPE,CONTRACT_DATE,COMMODITY,CONTRACT_REFERENCE]]>
		</queryString>
		<field name="COMPANY" class="java.lang.String"/>
		<field name="PROFIT_CENTER" class="java.lang.String"/>
		<field name="CONTRACT_REFERENCE" class="java.lang.String"/>
		<field name="CONTRACT_DATE" class="java.lang.String"/>
