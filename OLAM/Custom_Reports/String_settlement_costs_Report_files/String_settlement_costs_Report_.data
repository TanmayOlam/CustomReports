	<subDataset name="New Dataset 1" uuid="b84fa897-d517-49ec-8900-01ec3bca676e">
		<parameter name="asofdate" class="java.util.Date"/>
		<parameter name="ctrbook" class="java.lang.String"/>
		<parameter name="pc" class="java.util.Collection"/>
		<parameter name="commodity" class="java.util.Collection"/>
		<parameter name="drilldown_path" class="java.lang.String">
			<defaultValueExpression><![CDATA["/standard/Custom/Drilldown_Report/Cost_string_settlement_costs_report"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select  PROFIT_CENTER,CONTRACT_BOOK,POSITION_REF,COSTSHORTNAME
,sum(COST_AMOUNT_WITH_POL) as 'COST_AMOUNT_WITH_POL'
,sum(TOTAL_REALIZED_QUANTITY) as 'TOTAL_REALIZED_QUANTITY'
,avg(ACTUALPRICE) as 'ACTUALPRICE'
,avg(BUDGETPRICE) as 'BUDGET_PRICE'
from(
select PROFIT_CENTER,CONTRACT_BOOK,POSITION_REF,COSTSHORTNAME
,COST_AMOUNT *(-1) as 'COST_AMOUNT_WITH_POL'
,BUDGET_PRICE as BUDGETPRICE
,ACTUAL_PRICE
, isnull(ACTUAL_PRICE,0) *(-1) as 'ACTUALPRICE'
,TOTAL_REALIZED_QUANTITY
,COST_AMOUNT
,BUDGET_PRICE
from (
SELECT
       A.PROFIT_CENTER,A.LONG_SHORT,A.CONTRACT_BOOK,A.POSITION_REF,A.ALLOCATION_REF,A.PRICING_REF,A.CURRENT_POSITION_QUANTITY - A.CURRENT_OPEN_QUANTITY AS TOTAL_REALIZED_QUANTITY,
       COST.COSTSHORTNAME,C.COST_TYPE,
       C.CURRENT_BUDGET_PRICE * C.CURRENT_COSTTOHOME_FXRATE AS BUDGET_PRICE,
       CASE WHEN C.TOTAL_ACTUAL_AMOUNT_INHOME <> 0 THEN C.TOTAL_ACTUAL_AMOUNT_INHOME/A.allocation_qty ELSE NULL END AS ACTUAL_PRICE,
       (((C.CURRENT_BUDGET_PRICE * C.CURRENT_COSTTOHOME_FXRATE * C.CURRENT_COST_QUANTITY) + C.TOTAL_ACTUAL_AMOUNT_INHOME)/A.allocation_qty)
           * (A.CURRENT_POSITION_QUANTITY - A.CURRENT_OPEN_QUANTITY) AS COST_AMOUNT
FROM DAILY_COST_ATTRIBUTION C
JOIN cxcdata_olam47.DBO.JCCOST COST ON COST.COSTID = CAST(C.COST_ID AS INT)
JOIN DAILY_PNL_ATTRIBUTION A ON C.LOAD_ID = A.POSITION_ID AND C.ALLOCATION_ID = A.ALLOCATION_ID AND C.EOD_DATE = A.EOD_DATE
WHERE A.ALLOCATION_TYPE = 'STRING' AND A.CURRENT_OPEN_QUANTITY <> A.CURRENT_POSITION_QUANTITY
AND C.EOD_DATE =  $P{asofdate} AND C.COST_TYPE NOT IN ('FREIGHT','INSURANCE') AND C.IS_REALIZED_APPLICABLE = 1
  AND
  C.CONTRACT_BOOK= $P{ctrbook}
  AND $X{IN,A.PROFIT_CENTER,pc}
)T
)K
where  1 =case when ABS(K.ACTUALPRICE) = 0 and  ABS(K.BUDGET_PRICE) = 0 then 0 else 1 end

group by K.CONTRACT_BOOK,K.POSITION_REF, K.COSTSHORTNAME,K.PROFIT_CENTER


]]>
		</queryString>
		<field name="PROFIT_CENTER" class="java.lang.String"/>
		<field name="CONTRACT_BOOK" class="java.lang.String"/>
		<field name="POSITION_REF" class="java.lang.String"/>
		<field name="COSTSHORTNAME" class="java.lang.String"/>
		<field name="COST_AMOUNT_WITH_POL" class="java.math.BigDecimal"/>
		<field name="TOTAL_REALIZED_QUANTITY" class="java.math.BigDecimal"/>
		<field name="ACTUALPRICE" class="java.math.BigDecimal"/>
		<field name="BUDGET_PRICE" class="java.math.BigDecimal"/>
		<variable name="sum_POL" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{COST_AMOUNT_WITH_POL}]]></variableExpression>
		</variable>
		<variable name="sum_tot_qty" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TOTAL_REALIZED_QUANTITY}]]></variableExpression>
		</variable>
		<variable name="Tot_Avg_actual_price" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{ACTUALPRICE}]]></variableExpression>
		</variable>
		<variable name="Tot_But_Price" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{BUDGET_PRICE}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="ctrbook" class="java.lang.String"/>
	<parameter name="asofdate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new java.util.Date()-1]]></defaultValueExpression>
	</parameter>
	<parameter name="pc" class="java.util.Collection"/>
	<parameter name="commodity" class="java.util.Collection"/>
	<queryString>
		<![CDATA[SELECT Y.CONTRACT_BOOK
		,asofdate
		,(SELECT
			ISNULL(MAX(DPD.REPORT_DATE),'')
		  FROM DM_POSITION_DATA DPD
		  WHERE DPD.CONTRACT_BOOK = Y.CONTRACT_BOOK
		  AND DPD.REPORT_DATE <= Y.asofdate
		  AND $X{IN,DPD.PROFIT_CENTER,pc}
		  AND $X{IN,DPD.MATERIAL_GROUP ,commodity}
		  )
		  origindate
FROM (SELECT DISTINCT
		  DPA.CONTRACT_BOOK CONTRACT_BOOK,
		  DPA.EOD_DATE asofdate
		FROM DAILY_PNL_ATTRIBUTION DPA
		JOIN (SELECT MAX(EOD_DATE) EOD_DATE
				FROM DAILY_PNL_ATTRIBUTION b
				WHERE b.EOD_DATE <= $P{asofdate}
				AND $X{IN,b.PROFIT_CENTER,pc}
				AND $X{IN, b.COMMODITY , commodity}
				AND b.CONTRACT_BOOK like $P{ctrbook}
				) BVBV
		ON DPA.EOD_DATE = BVBV.EOD_DATE
		AND $X{IN,DPA.PROFIT_CENTER,pc}
		AND $X{IN,DPA.COMMODITY ,commodity}
		AND DPA.CONTRACT_BOOK like $P{ctrbook}
		UNION
		SELECT 'PALM' CONTRACT_BOOK, '1800-01-01' asofdate
		WHERE (SELECT COUNT(DISTINCT DPA.CONTRACT_BOOK)
				FROM DAILY_PNL_ATTRIBUTION DPA
				JOIN (SELECT MAX(EOD_DATE) EOD_DATE
						FROM DAILY_PNL_ATTRIBUTION b
						WHERE b.EOD_DATE <= $P{asofdate}
						AND $X{IN,b.PROFIT_CENTER,pc}
						AND $X{IN,b.COMMODITY ,commodity}
						AND b.CONTRACT_BOOK like $P{ctrbook}
						) BVBV
				ON DPA.EOD_DATE = BVBV.EOD_DATE
				AND $X{IN,DPA.PROFIT_CENTER,pc}
				AND $X{IN, DPA.COMMODITY, commodity}
				AND DPA.CONTRACT_BOOK like $P{ctrbook}
				) = 0
	) Y
WHERE 0 = 0]]>
	</queryString>
