		<queryString language="SQL">
			<![CDATA[SELECT CMP.COMPANY_NAME                                                             AS COMPANY,
       PC1.PROFIT_CENTER_GROUP                                                      AS PROFITCENTER_GROUP,
       USR.USER_NAME                                                                AS TRADER,
       ST.STRATEGY_NAME                                                             AS STRATEGY,
       PC1.PROFIT_CENTER_NAME                                                       AS PROFITCENTER,
       SHORT.COMMODITYID                                                            AS COMMODITY,
       CB1.CTRBOOKNAME                                                              AS CTRBOOK,
       LONG.CONTRACT_REFERENCE                                                      AS LONG_COVER_REF,
       LONG.EXTERNAL_REF1                                                           AS LONG_COVER_EXTERNAL_REF1,
       LONG.CONTRACT_DATE                                                           AS LONG_COVER_DATE,
       LONG.VALUE_END_DATE                                                          AS LONG_COVER_VALUE_END_DATE,
       LONG.INSTRUMENT_TYPE                                                         AS LONG_INSTRUMENT_TYPE,
       LONG.PRICE_CURRENCY                                                          AS LONG_COVER_TRANS_CURR,
       LONG.CONTRACT_CURRENCY                                                       AS LONG_COVER_FIN_CURR,
       A.MATCHED_AMOUNT                                                             AS LONG_COVER_AMOUNT_TRANS_CUR,
       A.MATCHED_AMOUNT * LONG.NET_RATE                                             AS LONG_COVER_AMOUNT_FIN_CURR,
       SHORT.CONTRACT_REFERENCE                                                     AS SHORT_COVER_REF,
       LONG.EXTERNAL_REF1                                                          AS LONG_COVER_EXTERNAL_REF1,
       SHORT.CONTRACT_DATE                                                          AS SHORT_COVER_DATE,
       SHORT.VALUE_END_DATE                                                         AS SHORT_COVER_VALUE_END_DATE,
       SHORT.PRICE_CURRENCY                                                         AS SHORT_COVER_TRANS_CURR,
       SHORT.CONTRACT_CURRENCY                                                      AS SHORT_COVER_FIN_CURR,
       A.MATCHED_AMOUNT                                                             AS SHORT_COVER_AMOUNT_TRANS_CURR,
       A.MATCHED_AMOUNT * SHORT.NET_RATE                                            AS SHORT_COVER_AMOUNT_FIN_CURR,
       ( A.MATCHED_AMOUNT * SHORT.NET_RATE ) - ( A.MATCHED_AMOUNT * LONG.NET_RATE ) AS REALISED_PNL_FIN_CURR,
       A.MATCH_DATE                                                                 AS DATE_FILTER,
       LONG.NET_RATE                                 AS  LONG_COVER_NET_RATE,
       SHORT.EXTERNAL_REF1                                                          AS SHORT_COVER_EXTERNAL_REF1,
        SHORT.NET_RATE                            AS SHORT_COVER_NET_RATE
FROM   BI_VIEW_CONTRACT_MATCHING A
       JOIN BI_VIEW_CONTRACT_FOREX_COVER SHORT
         ON A.SHORT_CTRID = SHORT.CTRID
       JOIN BI_VIEW_CONTRACT_FOREX_COVER LONG
         ON A.LONG_CTRID = LONG.CTRID
       JOIN BI_VIEW_PROFITCENTER PC1
         ON PC1.PFID = LONG.PFID
       JOIN BI_VIEW_STRATEGY ST
         ON ST.STRATEGYID = LONG.STRATEGYID
       JOIN BI_VIEW_USER USR
         ON USR.USERID = LONG.TRADERID
       JOIN BI_VIEW_COMPANY CMP
         ON CMP.COMPANYID = LONG.COMPANYID
       JOIN CTRBOOK CB1
         ON CB1.CTRBOOKID = SHORT.CTRBOOKID
WHERE  A.INSTRUMENT_TYPE = 'Forex'
      AND  $X{IN,PC1.PROFIT_CENTER_NAME,profitcenter}
        AND  $X{IN,SHORT.COMMODITYID,product}
        AND  $X{IN,LONG.PRICE_CURRENCY ,currency}
        AND LONG.CONTRACT_DATE >=  $P{from_cover_date}
        AND LONG.CONTRACT_DATE <=  $P{to_cover_date}
        AND $X{IN,LONG.INSTRUMENT_TYPE ,df_ndf}
        AND $X{IN,CB1.CTRBOOKNAME ,ctrbook}
        AND $X{IN,PC1.PROFIT_CENTER_GROUP,profitcenter_group}
        AND $X{IN,ST.STRATEGY_NAME ,strategy}
     AND $X{IN,USR.USER_NAME ,trader}
     AND $X{IN,CMP.COMPANY_NAME,company}
      AND A.MATCH_DATE >=  $P{netoff_from_date}
      AND A.MATCH_DATE <= $P{netoff_to_date}
       $P!{QSTR_Filter}
ORDER  BY 1,
          2,
          8]]>
		</queryString>
