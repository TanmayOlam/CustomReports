		<queryString>
			<![CDATA[SELECT * FROM (SELECT FX.FXCTRID                                          AS CTRID,
       FX.COMMODITYID                                      AS PROD,
       PF.PCID,
       PF.PCNAME                                           AS UNIT,
       FT.FXCTRTYPESHORTNAME                               AS CONTRACT_TYPE,
       CASE
         WHEN FX.COMPANYID = FX.SLRID THEN 'Short'
         ELSE 'Long'
       END                                                 AS LONG_SHORT,
       FX.BASECURR + '-' + FX.VALUECURR                    AS SELL_BUY,
       FX.FXCTRREF                                         AS CXC_CV_NO,
       FX.EXTERNALREF1                                     AS CC_CV_NO,
       CASE
         WHEN FX.ISINTERNAL = 0
              AND CP.MBRSHORTNAME <> '(N/A)'
              AND FT.FXCTRTYPESHORTNAME = 'BANK'
              AND BKR.MBRSHORTNAME = '(DIRECT)' THEN 'BANK'
         WHEN FX.ISINTERNAL = 0
              AND CP.MBRSHORTNAME = '(N/A)'
              AND FT.FXCTRTYPESHORTNAME = 'BANK'
              AND BKR.MBRSHORTNAME <> '(DIRECT)' THEN 'BROKER'
         WHEN FX.ISINTERNAL = 1
              AND FT.FXCTRTYPESHORTNAME = 'INTERNAL' THEN 'INTERNAL'
         ELSE 'OTHERS'
       END                                     AS TYPE,
       FX.FXCTRDATE                                        AS CV_DATE,
       FX.VALIDITYSTART                                    AS VAL_DATE,
       FX.FXCTRSPOT + FX.FXCTRSWAP                         AS EXCH_RATE,
       FX.BASECURR                                         AS SELL_CURRENCY,
       FX.QUANTITY * CASE
                       WHEN FX.COMPANYID = FX.SLRID THEN -1
                       ELSE 1
                     END                                   AS SELL_AMOUNT,
       FX.VALUECURR                                        AS BUY_CURRENCY,
       FX.QUANTITY * ( FX.FXCTRSPOT + FX.FXCTRSWAP ) * CASE
                                                         WHEN FX.COMPANYID = FX.SLRID THEN 1
                                                         ELSE -1
                                                       END AS BUY_AMOUNT,
       CASE
         WHEN FX.ISSPLIT = 1 THEN FX.QUANTITY
         ELSE FXCS.SUMAMT
       END                                                 AS ADJ_AMOUNT,
       CASE
         WHEN FX.COMPANYID = FX.SLRID THEN FXCS.REFB
         ELSE FXCS.REFS
       END                                                 AS ADJ_CONTRACT,
       NULL                                                AS TFR_AMOUNT,
       FXCS.ID                                             AS FXCLOSEID,
       ( FX.QUANTITY - CASE
                                WHEN FX.ISSPLIT = 1 THEN FX.QUANTITY
                                ELSE COALESCE(APP.APPLIED_AMT_TRANS, 0)
                              END )
       * CASE
               WHEN FX.COMPANYID = FX.SLRID THEN -1
               ELSE 1
             END                                           AS BALANCE_SELL_AMOUNT,
       ( FX.QUANTITY - CASE
                                WHEN FX.ISSPLIT = 1 THEN FX.QUANTITY
                                ELSE COALESCE(APP.APPLIED_AMT_TRANS, 0)
                              END ) * ( FX.FXCTRSPOT + FX.FXCTRSWAP )
       * CASE
               WHEN FX.COMPANYID = FX.SLRID THEN 1
               ELSE -1
             END                                           AS BALANCE_BUY_AMOUNT,
       CASE WHEN Row_number()
                                   OVER(
                                     PARTITION BY FX.FXCTRID
                                     ORDER BY FX.FXCTRID) = 1 THEN 1 ELSE 0 END                                                 AS FLAG
FROM   FXCTR FX
       LEFT JOIN FXTRANSFER TRANS
              ON FX.FXCTRID = TRANS.FXCTRID
       JOIN JCMBR CP
         ON CASE
              WHEN FX.BYRID = FX.COMPANYID THEN FX.SLRID
              ELSE FX.BYRID
            END = CP.MBRID
       JOIN JCMBR BKR
         ON FX.BKRID = BKR.MBRID
       JOIN JCPROFITCENTER PF
         ON FX.PCID = PF.PCID
       JOIN XSFXCTRSTATUS STATS
         ON FX.FXCTRSTATUSID = STATS.FXCTRSTATUSID
       JOIN GLCOMPANY GL
         ON FX.COMPANYID = GL.COMPANYID
     JOIN JCMBR COMP
       ON GL.COMPANYID = COMP.MBRID
       JOIN CTRBOOK CBK
         ON FX.CTRBOOKID = CBK.CTRBOOKID
       JOIN JCPROFITCENTER PC
         ON FX.PCID = PC.PCID
       LEFT JOIN JCPROFITCENTERGROUP PCGRP
              ON PC.PCGROUPID = PCGRP.GROUPID
       JOIN JCSTRATEGY STRAT
         ON FX.STRATEGYID = STRAT.STRATEGYID
       JOIN JC_USER TRADER
         ON FX.TRADERID = TRADER.ID
       LEFT JOIN JCCPGRP CPGRP
              ON CP.CPGRPID = CPGRP.CPGRPID
       LEFT JOIN JCCOUNTERPARTYCATEGORY CPCATE
              ON CP.CPCATEID = CPCATE.CPCATEID
       LEFT JOIN JCFXCTRTYPE FT
              ON FT.FXCTRTYPEID = FX.FXCTRTYPEID
       JOIN XSFXCTRSTATUS STA
         ON FX.FXCTRSTATUSID = STA.FXCTRSTATUSID
            AND FX.FXCTRSTATUSID IN ( 1, 2, 3, 10)
       LEFT JOIN (SELECT FXX.SLRFXCTRID,
                         FXX.FXCLOSEOUTID               AS ID,
                         AMOUNT                         SUMAMT,
                         FX.FXCTRREF                    AS REFS,
                         FXB.FXCTRREF                   AS REFB,
                         FXX.BYRFXCTRID,
                         Row_number()
                           OVER(
                             PARTITION BY FXX.SLRFXCTRID
                             ORDER BY FXX.FXCLOSEOUTID) AS Row_SALES,
                         Row_number()
                           OVER(
                             PARTITION BY FXX.BYRFXCTRID
                             ORDER BY FXX.FXCLOSEOUTID) AS Row_PURCHASE
                  FROM   FXCLOSEOUTBYCTR FXX
                         LEFT JOIN FXCTR FX
                                ON FXX.SLRFXCTRID = FX.FXCTRID
                         LEFT JOIN FXCTR FXB
                                ON FXX.BYRFXCTRID = FXB.FXCTRID
                  UNION
                  SELECT FXCTRID,
                         AR.FXCLOSEOUTID               AS ID,
                         AMOUNT                        SUMAMT,
                         ARH.ARREF                     AS REFS,
                         ARH.ARREF                     AS REFB,
                         FXCTRID,
                         Row_number()
                           OVER(
                             PARTITION BY FXCTRID
                             ORDER BY AR.FXCLOSEOUTID) AS Row_SALES,
                         NULL
                  FROM   FXCLOSEOUTBYAR AR
                         LEFT JOIN ARHEADER ARH
                                ON AR.ARID = ARH.ARID
                  UNION
                  SELECT FXCTRID,
                         AP.FXCLOSEOUTID               AS ID,
                         AMOUNT                        SUMAMT,
                         APH.APREF                     AS REFS,
                         APH.APREF                     AS REFB,
                         FXCTRID,
                         NULL,
                         Row_number()
                           OVER(
                             PARTITION BY FXCTRID
                             ORDER BY AP.FXCLOSEOUTID) AS Row_PURCHASE
                  FROM   FXCLOSEOUTBYAP AP
                         LEFT JOIN APHEADER APH
                                ON AP.APID = APH.APID) FXCS
              ON FX.FXCTRID = CASE
                                WHEN FX.COMPANYID = FX.SLRID THEN FXCS.SLRFXCTRID
                                ELSE FXCS.BYRFXCTRID
                              END
       LEFT JOIN (SELECT L1.CTRID,
                         Sum(L1.APPLIED_AMT_TRANS) AS APPLIED_AMT_TRANS
                  FROM   (SELECT F.FXCTRID                                    AS CTRID,
                                 FXCTRDATE                                    AS DATE,
                                 NULL                                         AS APPLYID,
                                 QUANTITY                                     AS AMOUNT,
                                 NULL                                         AS APPLIED_AMT_TRANS,
                                 'CONTRACT'                                   AS TYPE,
                                 (SELECT Max(v)
                                  FROM   (VALUES (F.CREATETIME),
                                                 (F.UPDATETIME)) AS value(v)) AS LASTUPDATE
                          FROM   FXCTR F
                          UNION
                          SELECT F.FXCTRID                                     AS CTRID,
                                 CLOSEDDATE                                    AS DATE,
                                 FXCLOSEOUTID                                  AS APPLYID,
                                 QUANTITY                                      AS AMOUNT,
                                 AR.AMOUNT                                     AS APPLIED_AMT_TRANS,
                                 'INVOICE'                                     AS TYPE,
                                 (SELECT Max(v)
                                  FROM   (VALUES (F.CREATETIME),
                                                 (F.UPDATETIME),
                                                 (AR.CLOSEDDATE)) AS value(v)) AS LASTUPDATE
                          FROM   FXCTR F
                                 JOIN FXCLOSEOUTBYAR AR
                                   ON F.FXCTRID = AR.FXCTRID
                          UNION
                          SELECT F.FXCTRID                                     AS CTRID,
                                 CLOSEDDATE                                    AS DATE,
                                 FXCLOSEOUTID                                  AS APPLYID,
                                 QUANTITY                                      AS AMOUNT,
                                 AP.AMOUNT                                     AS APPLIED_AMT_TRANS,
                                 'VOUCHER'                                     AS TYPE,
                                 (SELECT Max(v)
                                  FROM   (VALUES (F.CREATETIME),
                                                 (F.UPDATETIME),
                                                 (AP.CLOSEDDATE)) AS value(v)) AS LASTUPDATE
                          FROM   FXCTR F
                                 JOIN FXCLOSEOUTBYAP AP
                                   ON F.FXCTRID = AP.FXCTRID
                          UNION
                          SELECT F.FXCTRID                                    AS CTRID,
                                 B.CLOSEDDATE                                 AS DATE,
                                 C.FXCLOSEOUTID                               AS APPLYID,
                                 QUANTITY                                     AS AMOUNT,
                                 C.AMOUNT                                     AS APPLIED_AMT_TRANS,
                                 'CLOSE SHORT'                                AS TYPE,
                                 (SELECT Max(v)
                                  FROM   (VALUES (F.CREATETIME),
                                                 (F.UPDATETIME),
                                                 (B.CLOSEDDATE)) AS value(v)) AS LASTUPDATE
                          FROM   FXCTR F
                                 JOIN FXCLOSEOUTBYCTR C
                                   ON C.BYRFXCTRID = F.FXCTRID
                                 LEFT JOIN FXCLOSEOUT B
                                        ON C.FXCLOSEOUTID = B.FXCLOSEOUTID
                          UNION
                          SELECT F.FXCTRID                                    AS CTRID,
                                 B.CLOSEDDATE                                 AS DATE,
                                 C.FXCLOSEOUTID                               AS APPLYID,
                                 QUANTITY                                     AS AMOUNT,
                                 C.AMOUNT                                     AS APPLIED_AMT_TRANS,
                                 'CLOSE LONG'                                 AS TYPE,
                                 (SELECT Max(v)
                                  FROM   (VALUES (F.CREATETIME),
                                                 (F.UPDATETIME),
                                                 (B.CLOSEDDATE)) AS value(v)) AS LASTUPDATE
                          FROM   FXCTR F
                                 JOIN FXCLOSEOUTBYCTR C
                                   ON C.SLRFXCTRID = F.FXCTRID
                                 LEFT JOIN FXCLOSEOUT B
                                        ON C.FXCLOSEOUTID = B.FXCLOSEOUTID) L1
                  GROUP  BY CTRID) APP
              ON FX.FXCTRID = APP.CTRID
WHERE  STATS.FXCTRSTATUSID IN ( 1, 2, 3, 10 )
       AND ( CASE WHEN STATS.FXCTRSTATUSID = 2 OR STATS.FXCTRSTATUSID = 3 THEN FX.ISSPLIT ELSE 1 END= 1
              OR CASE WHEN STATS.FXCTRSTATUSID = 2 OR STATS.FXCTRSTATUSID =3 THEN FX.ISACTIVE ELSE 1 END = 1 )
      AND CASE WHEN $P{dateselection} = 1 THEN FX.FXCTRDATE ELSE FX.VALIDITYSTART END >= $P{displayfromdate}
       AND CASE WHEN $P{dateselection} = 1 THEN FX.FXCTRDATE ELSE FX.VALIDITYSTART END <=  $P{displaytodate}
AND $X{IN, STATS.STATUSDESCN, status}
       AND CASE
               WHEN FX.COMPANYID = FX.SLRID THEN 'Short'
               ELSE 'Long'
             END LIKE $P{longshort}
       AND CASE
               WHEN FX.ISDELIVERABLE = 1 THEN 'Deliverable'
               ELSE 'Non-Deliverable'
             END LIKE $P{instrument}
       AND COMP.MBRNAME LIKE $P{company}
       AND CBK.CTRBOOKCODE LIKE $P{cbk}
       AND PC.PCNAME LIKE $P{pc}
       AND STRAT.NAME LIKE $P{strategy}
       AND TRADER.FIRST_NAME + ' ' + COALESCE(TRADER.LAST_NAME, '') LIKE $P{trader}
       AND CP.MBRSHORTNAME LIKE $P{cp}
       AND CASE WHEN CP.ISINTERNAL = 1 THEN 'Internal' ELSE 'External' END LIKE $P{intext}
       AND BKR.MBRSHORTNAME LIKE $P{broker}
       AND FX.COMMODITYID LIKE $P{cmd}
       AND COALESCE(PCGRP.SHORTNAME, '%') LIKE $P{pcgrp}
       AND COALESCE(CPGRP.SHORTNAME, '%') LIKE $P{cpgrp}
       AND COALESCE(CPCATE.NAME, '%') LIKE $P{cpcat}
       AND ABS(CASE
             WHEN TRANS.FXTXID IS NOT NULL THEN FX.QUANTITY * ( FX.FXCTRSPOT + FX.FXCTRSWAP )
             ELSE ( FX.QUANTITY - CASE
                                    WHEN FX.ISSPLIT = 1 THEN FX.QUANTITY
                                    ELSE COALESCE(APP.APPLIED_AMT_TRANS, 0)
                                  END ) * ( FX.FXCTRSPOT + FX.FXCTRSWAP )
           END) > CASE
                   WHEN $P{ctrbal} = 1 THEN -1
                   ELSE 0
                 END
     $P!{QSTR_Filter}
UNION
SELECT FX.FXCTRID                                          AS CTRID,
       FX.COMMODITYID                                      AS PROD,
       PF.PCID,
       PF.PCNAME                                           AS UNIT,
       FT.FXCTRTYPESHORTNAME                               AS CONTRACT_TYPE,
       CASE
         WHEN FX.COMPANYID = FX.SLRID THEN 'Short'
         ELSE 'Long'
       END                                                 AS LONG_SHORT,
       FX.BASECURR + '-' + FX.VALUECURR                    AS SELL_BUY,
       FX.FXCTRREF                                         AS CXC_CV_NO,
       FX.EXTERNALREF1                                     AS CC_CV_NO,
       CASE
         WHEN FX.ISINTERNAL = 0
              AND CP.MBRSHORTNAME <> '(N/A)'
              AND FT.FXCTRTYPESHORTNAME = 'BANK'
              AND BKR.MBRSHORTNAME = '(DIRECT)' THEN 'BANK'
         WHEN FX.ISINTERNAL = 0
              AND CP.MBRSHORTNAME = '(N/A)'
              AND FT.FXCTRTYPESHORTNAME = 'BANK'
              AND BKR.MBRSHORTNAME <> '(DIRECT)' THEN 'BROKER'
         WHEN FX.ISINTERNAL = 1
              AND FT.FXCTRTYPESHORTNAME = 'INTERNAL' THEN 'INTERNAL'
         ELSE 'OTHERS'
       END                                     AS TYPE,
       FX.FXCTRDATE                                        AS CV_DATE,
       FX.VALIDITYSTART                                    AS VAL_DATE,
       FX.FXCTRSPOT + FX.FXCTRSWAP                         AS EXCH_RATE,
       FX.BASECURR                                         AS SELL_CURRENCY,
       FX.QUANTITY * CASE
                       WHEN FX.COMPANYID = FX.SLRID THEN -1
                       ELSE 1
                     END                                   AS SELL_AMOUNT,
       FX.VALUECURR                                        AS BUY_CURRENCY,
       FX.QUANTITY * ( FX.FXCTRSPOT + FX.FXCTRSWAP ) * CASE
                                                         WHEN FX.COMPANYID = FX.SLRID THEN 1
                                                         ELSE -1
                                                       END AS BUY_AMOUNT,
       NULL                                                AS ADJ_AMOUNT,
       NULL                                                AS ADJ_CONTRACT,
       FX.QUANTITY                                         AS TFR_AMOUNT,
       NULL                                                AS FXCLOSEID,
       NULL                                                AS BALANCE_SELL_AMOUNT,
       NULL                                                AS BALANCE_BUY_AMOUNT,
     1 AS FLAG
FROM   FXCTR FX
       JOIN JCMBR CP
         ON CASE
              WHEN FX.BYRID = FX.COMPANYID THEN FX.SLRID
              ELSE FX.BYRID
            END = CP.MBRID
       JOIN FXTRANSFER TRANS
         ON FX.FXCTRID = TRANS.FXCTRID
       JOIN JCMBR BKR
         ON FX.BKRID = BKR.MBRID
       JOIN JCPROFITCENTER PF
         ON TRANS.PCFROM = PF.PCID
       JOIN XSFXCTRSTATUS STATS
         ON FX.FXCTRSTATUSID = STATS.FXCTRSTATUSID
       JOIN GLCOMPANY GL
         ON FX.COMPANYID = GL.COMPANYID
     JOIN JCMBR COMP
       ON GL.COMPANYID = COMP.MBRID
       JOIN CTRBOOK CBK
         ON FX.CTRBOOKID = CBK.CTRBOOKID
       JOIN JCPROFITCENTER PC
         ON TRANS.PCFROM = PC.PCID
       LEFT JOIN JCPROFITCENTERGROUP PCGRP
              ON PC.PCGROUPID = PCGRP.GROUPID
       JOIN JCSTRATEGY STRAT
         ON FX.STRATEGYID = STRAT.STRATEGYID
       JOIN JC_USER TRADER
         ON FX.TRADERID = TRADER.ID
       LEFT JOIN JCCPGRP CPGRP
              ON CP.CPGRPID = CPGRP.CPGRPID
       LEFT JOIN JCCOUNTERPARTYCATEGORY CPCATE
              ON CP.CPCATEID = CPCATE.CPCATEID
       LEFT JOIN JCFXCTRTYPE FT
              ON FT.FXCTRTYPEID = FX.FXCTRTYPEID
       JOIN XSFXCTRSTATUS STA
         ON FX.FXCTRSTATUSID = STA.FXCTRSTATUSID
            AND FX.FXCTRSTATUSID IN ( 1, 2, 3, 10)
WHERE  STATS.FXCTRSTATUSID IN ( 1, 2, 3, 10 )
       AND ( CASE WHEN STATS.FXCTRSTATUSID = 2 OR STATS.FXCTRSTATUSID = 3 THEN FX.ISSPLIT ELSE 1 END= 1
              OR CASE WHEN STATS.FXCTRSTATUSID = 2 OR STATS.FXCTRSTATUSID =3 THEN FX.ISACTIVE ELSE 1 END = 1 )
    AND CASE WHEN $P{dateselection} = 1 THEN FX.FXCTRDATE ELSE FX.VALIDITYSTART END >= $P{displayfromdate}
       AND CASE WHEN $P{dateselection} = 1 THEN FX.FXCTRDATE ELSE FX.VALIDITYSTART END <= $P{displaytodate}
     AND $X{IN, STATS.STATUSDESCN, status}
     AND CASE
             WHEN FX.COMPANYID = FX.SLRID THEN 'Short'
             ELSE 'Long'
           END LIKE $P{longshort}
     AND CASE
             WHEN FX.ISDELIVERABLE = 1 THEN 'Deliverable'
             ELSE 'Non-Deliverable'
           END LIKE $P{instrument}
     AND COMP.MBRNAME LIKE $P{company}
     AND CBK.CTRBOOKCODE LIKE $P{cbk}
     AND PC.PCNAME LIKE $P{pc}
     AND STRAT.NAME LIKE $P{strategy}
     AND TRADER.FIRST_NAME + ' ' + COALESCE(TRADER.LAST_NAME, '') LIKE $P{trader}
     AND CP.MBRSHORTNAME LIKE $P{cp}
     AND CASE WHEN CP.ISINTERNAL = 1 THEN 'Internal' ELSE 'External' END LIKE $P{intext}
     AND BKR.MBRSHORTNAME LIKE $P{broker}
     AND FX.COMMODITYID LIKE $P{cmd}
     AND COALESCE(PCGRP.SHORTNAME, '%') LIKE $P{pcgrp}
     AND COALESCE(CPGRP.SHORTNAME, '%') LIKE $P{cpgrp}
     AND COALESCE(CPCATE.NAME, '%') LIKE $P{cpcat}
     AND $P{ctrbal} = 1
     $P!{QSTR_Filter}
) A
WHERE $X{IN,A.TYPE,ctrtype}
ORDER  BY A.PROD,
      A.UNIT,
      A.SELL_CURRENCY]]>
		</queryString>
